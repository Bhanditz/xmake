# preifx
include $(PRO_DIR)/prefix.mak

# module name
NAMES 											= demo

# type
demo_TYPE 										= BIN

# files
demo_C_FILES 									+= xmake

# packages
demo_PKGS-$(TBOX)  								+= tbox 
